{
  "compilerOptions": {
    "target": "ES2021",
    "module": "es2022",
    "allowJs": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "lib": [
      "es2021",
      "dom",
      "webworker"
    ]
  }
}
